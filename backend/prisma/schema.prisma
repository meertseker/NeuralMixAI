generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model BeatProperties {
  id              Int      @id @default(autoincrement())
  genrePrediction String
  createdAt       DateTime @default(now())
  updatedAt       DateTime
  beat            String
}

model Plugin {
  id            Int             @id @default(autoincrement())
  presetId      Int
  pluginName    String
  createdAt     DateTime        @default(now())
  updatedAt     DateTime
  Preset        Preset          @relation(fields: [presetId], references: [id])
  PluginSetting PluginSetting[]
}

model PluginSetting {
  id                 Int      @id @default(autoincrement())
  pluginId           Int
  pluginSettingName  String
  pluginSettingValue String
  createdAt          DateTime @default(now())
  updatedAt          DateTime
  Plugin             Plugin   @relation(fields: [pluginId], references: [id])
}

model Preset {
  id           Int        @id @default(autoincrement())
  vocalChainId Int
  presetName   String
  createdAt    DateTime   @default(now())
  updatedAt    DateTime
  Plugin       Plugin[]
  VocalChain   VocalChain @relation(fields: [vocalChainId], references: [id])
}

model UserInput {
  id          Int    @id @default(autoincrement())
  userId      String
  description String
  beatUrl     String
  AudioUrl    String
}

model VocalChain {
  id             Int      @id @default(autoincrement())
  userId         String
  vocalChainName String
  createdAt      DateTime @default(now())
  updatedAt      DateTime
  Preset         Preset[]
}
